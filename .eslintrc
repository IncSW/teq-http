{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "restParams": true,
        "spread": true,
        "templateStrings": true
    },
    "env": {
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": [2, "never"],
        "no-console": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": 2,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": [2, {"requireParamDescription": false, "requireReturnDescription": false}],
        "valid-typeof": 2,
        "block-scoped-var": 2,
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-location": [2, "object"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-extend-native": [2, {"exceptions": ["Object"]}],
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-invalid-this": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": 2,
        "no-multi-spaces": 2,
        "no-native-reassign": [2, {"exceptions": ["Object"]}],
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "yoda": 2,
        "no-shadow-restricted-names": 2,
        "no-undef-init": 2,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "array-bracket-spacing": [2, "never"],
        "brace-style": 2,
        "camelcase": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "computed-property-spacing": [2, "never"],
        "consistent-this": [2, "self"],
        "indent": [2, 4, {"SwitchCase": 1}],
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "linebreak-style": [2, "unix"],
        "no-array-constructor": 2,
        "no-new-object": 2,
        "one-var": [2, "never"],
        "quotes": [2, "single"],
        "require-jsdoc": 2,
        "semi-spacing": 2,
        "semi": 2,
        "space-after-keywords": 2,
        "space-return-throw-case": 2,
        "constructor-super": 2,
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-var": 2,
        "object-shorthand": 2,
        "prefer-arrow-callback": 2,
        "require-yield": 2
    }
}